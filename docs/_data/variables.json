          "default": "0.75rem"
        },
        {
          "coefficient": 1,
          "default": "1rem"
        },
        {
          "coefficient": 1.25,
          "default": "1.25rem"
        },
        {
          "coefficient": 1.5,
          "default": "1.5rem"
        },
        {
          "coefficient": 1.75,
          "default": "1.75rem"
        },
        {
          "coefficient": 2,
          "default": "2rem"
        },
        {
          "coefficient": 2.5,
          "default": "2.5rem"
        },
        {
          "coefficient": 3,
          "default": "3rem"
        },
        {
          "coefficient": 4,
          "default": "4rem"
        },
        {
          "coefficient": 5,
          "default": "5rem"
        },
        {
          "coefficient": 6,
          "default": "6rem"
        },
        {
          "coefficient": 7,
          "default": "7rem"
        },
        {
          "coefficient": 8,
          "default": "8rem"
        }
      ],
      "headers": [
        {
          "name": "Spacing index",
          "prop": "NA",
          "type": "index"
        },
        {
          "name": "Coefficient",
          "prop": "coefficient",
          "type": "integer"
        },
        {
          "name": "Computed value",
          "prop": "default",
          "type": "string"
        }
      ]
    }
  },

  "typography": {
    "fontTracking": {
      "list": [
        {
          "class": ".tracking-tight",
          "property": "letter-spacing: -0.05em"
        },
        {
          "class": ".tracking-close",
          "property": "letter-spacing: -0.05em"
        },
        {
          "class": ".tracking-normal",
